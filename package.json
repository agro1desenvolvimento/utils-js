  "engines": {
    "node": ">=12 <=14"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@types/lodash": "^4.14.165",
    "lodash": "^4.17.20",
    "typescript": "^4.1.3"
